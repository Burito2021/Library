          "title": "System Load",
          "type": "graph",
          "yAxes": [{"format": "short", "min": 0}]
        },
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 15,
          "legend": {"show": true},
          "lines": true,
          "span": 3,
          "targets": [
            {"expr": "process_files_open_files", "legendFormat": "Open", "refId": "A"},
            {"expr": "process_files_max_files", "legendFormat": "Max", "refId": "B"}
          ],
          "title": "File Descriptors",
          "type": "graph",
          "yAxes": [{"format": "short", "min": 0}]
        }
      ],
      "showTitle": true,
      "title": "System Metrics",
      "titleSize": "h6"
    },
    {
      "collapse": false,
      "height": 250,
      "panels": [
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 16,
          "legend": {"show": true},
          "lines": true,
          "span": 4,
          "targets": [
            {"expr": "rate(jvm_gc_pause_seconds_count[1m])", "legendFormat": "{{action}} ({{cause}})", "refId": "A"}
          ],
          "title": "GC Collections",
          "type": "graph",
          "yAxes": [{"format": "ops", "min": 0}]
        },
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 17,
          "legend": {"show": true},
          "lines": true,
          "span": 4,
          "targets": [
            {"expr": "rate(jvm_gc_pause_seconds_sum[1m])/rate(jvm_gc_pause_seconds_count[1m])", "legendFormat": "Avg {{action}}", "refId": "A"},
            {"expr": "jvm_gc_pause_seconds_max", "legendFormat": "Max {{action}}", "refId": "B"}
          ],
          "title": "GC Pause Duration",
          "type": "graph",
          "yAxes": [{"format": "s", "min": 0}]
        },
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 18,
          "legend": {"show": true},
          "lines": true,
          "span": 4,
          "targets": [
            {"expr": "rate(jvm_gc_memory_allocated_bytes_total[1m])", "legendFormat": "Allocated", "refId": "A"},
            {"expr": "rate(jvm_gc_memory_promoted_bytes_total[1m])", "legendFormat": "Promoted", "refId": "B"}
          ],
          "title": "GC Memory",
          "type": "graph",
          "yAxes": [{"format": "Bps", "min": 0}]
        }
      ],
      "showTitle": true,
      "title": "Garbage Collection",
      "titleSize": "h6"
    },
    {
      "collapse": false,
      "height": 250,
      "panels": [
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 19,
          "legend": {"show": true},
          "lines": true,
          "span": 6,
          "targets": [
            {"expr": "jvm_classes_loaded_classes", "legendFormat": "Loaded Classes", "refId": "A"}
