{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "limit": 100,
        "name": "Annotations & Alerts",
        "showIn": 0,
        "type": "dashboard"
      },
      {
        "datasource": "Prometheus",
        "enable": true,
        "expr": "resets(process_uptime_seconds[1m]) > 0",
        "iconColor": "rgba(255, 96, 96, 1)",
        "name": "Restart Detection",
        "showIn": 0,
        "step": "1m",
        "tagKeys": "restart-tag",
        "textFormat": "uptime reset",
        "titleFormat": "Restart"
      }
    ]
  },
  "description": "Simplified JVM Dashboard for Library REST API",
  "editable": true,
  "id": null,
  "links": [],
  "refresh": "30s",
  "rows": [
    {
      "collapse": false,
      "height": "100px",
      "panels": [
        {
          "colorBackground": false,
          "colorValue": true,
          "colors": ["rgba(245, 54, 54, 0.9)", "rgba(237, 129, 40, 0.89)", "rgba(50, 172, 45, 0.97)"],
          "datasource": "Prometheus",
          "format": "s",
          "id": 1,
          "span": 3,
          "targets": [{"expr": "process_uptime_seconds", "legendFormat": "", "refId": "A"}],
          "title": "Uptime",
          "type": "singlestat",
          "valueName": "current"
        },
        {
          "colorBackground": false,
          "colorValue": true,
          "colors": ["rgba(245, 54, 54, 0.9)", "rgba(237, 129, 40, 0.89)", "rgba(50, 172, 45, 0.97)"],
          "datasource": "Prometheus",
          "format": "dateTimeAsIso",
          "id": 2,
          "span": 3,
          "targets": [{"expr": "process_start_time_seconds*1000", "legendFormat": "", "refId": "A"}],
          "title": "Start Time",
          "type": "singlestat",
          "valueName": "current"
        },
        {
          "colorBackground": false,
          "colorValue": true,
          "colors": ["rgba(50, 172, 45, 0.97)", "rgba(237, 129, 40, 0.89)", "rgba(245, 54, 54, 0.9)"],
          "datasource": "Prometheus",
          "format": "percent",
          "id": 3,
          "span": 3,
          "targets": [{"expr": "sum(jvm_memory_used_bytes{area=\"heap\"})*100/sum(jvm_memory_max_bytes{area=\"heap\"})", "legendFormat": "", "refId": "A"}],
          "thresholds": "70,90",
          "title": "Heap Usage",
          "type": "singlestat",
          "valueName": "current"
        },
        {
          "colorBackground": false,
          "colorValue": true,
          "colors": ["rgba(50, 172, 45, 0.97)", "rgba(237, 129, 40, 0.89)", "rgba(245, 54, 54, 0.9)"],
          "datasource": "Prometheus",
          "format": "percent",
          "id": 4,
          "span": 3,
          "targets": [{"expr": "sum(jvm_memory_used_bytes{area=\"nonheap\"})*100/sum(jvm_memory_max_bytes{area=\"nonheap\"})", "legendFormat": "", "refId": "A"}],
          "thresholds": "70,90",
          "title": "Non-Heap Usage",
          "type": "singlestat",
          "valueName": "current"
        }
      ],
      "showTitle": true,
      "title": "Overview",
      "titleSize": "h6"
    },
    {
      "collapse": false,
      "height": 250,
      "panels": [
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 5,
          "legend": {"show": true},
          "lines": true,
          "span": 4,
          "targets": [{"expr": "sum(rate(http_server_requests_seconds_count[1m]))", "legendFormat": "Requests/sec", "refId": "A"}],
          "title": "HTTP Rate",
          "type": "graph",
          "yAxes": [{"format": "ops", "min": 0}]
        },
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 6,
          "legend": {"show": true},
          "lines": true,
          "span": 4,
          "targets": [
            {"expr": "sum(rate(http_server_requests_seconds_sum[1m]))/sum(rate(http_server_requests_seconds_count[1m]))", "legendFormat": "Avg", "refId": "A"},
            {"expr": "max(http_server_requests_seconds_max{status!~\"5..\"})", "legendFormat": "Max", "refId": "B"}
          ],
          "title": "HTTP Duration",
          "type": "graph",
          "yAxes": [{"format": "s", "min": 0}]
        },
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 8,
          "legend": {"show": true},
          "lines": true,
          "span": 4,
          "targets": [
            {"expr": "system_cpu_usage", "legendFormat": "System", "refId": "A"},
            {"expr": "process_cpu_usage", "legendFormat": "Process", "refId": "B"}
          ],
          "title": "CPU Usage",
          "type": "graph",
          "yAxes": [{"format": "percentunit", "max": 1, "min": 0}]
        }
      ],
      "showTitle": true,
      "title": "Application Metrics",
      "titleSize": "h6"
    },
    {
      "collapse": false,
      "height": 250,
      "panels": [
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 9,
          "legend": {"show": true},
          "lines": true,
          "span": 4,
          "targets": [
            {"expr": "sum(jvm_memory_used_bytes{area=\"heap\"})", "legendFormat": "Used", "refId": "A"},
            {"expr": "sum(jvm_memory_committed_bytes{area=\"heap\"})", "legendFormat": "Committed", "refId": "B"},
            {"expr": "sum(jvm_memory_max_bytes{area=\"heap\"})", "legendFormat": "Max", "refId": "C"}
          ],
          "title": "JVM Heap Memory",
          "type": "graph",
          "yAxes": [{"format": "bytes", "min": 0}]
        },
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 10,
          "legend": {"show": true},
          "lines": true,
          "span": 4,
          "targets": [
            {"expr": "sum(jvm_memory_used_bytes{area=\"nonheap\"})", "legendFormat": "Used", "refId": "A"},
            {"expr": "sum(jvm_memory_committed_bytes{area=\"nonheap\"})", "legendFormat": "Committed", "refId": "B"},
            {"expr": "sum(jvm_memory_max_bytes{area=\"nonheap\"})", "legendFormat": "Max", "refId": "C"}
          ],
          "title": "JVM Non-Heap Memory",
          "type": "graph",
          "yAxes": [{"format": "bytes", "min": 0}]
        },
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 11,
          "legend": {"show": true},
          "lines": true,
          "span": 4,
          "targets": [
            {"expr": "sum(jvm_memory_used_bytes)", "legendFormat": "Used", "refId": "A"},
            {"expr": "sum(jvm_memory_committed_bytes)", "legendFormat": "Committed", "refId": "B"},
            {"expr": "sum(jvm_memory_max_bytes)", "legendFormat": "Max", "refId": "C"}
          ],
          "title": "JVM Total Memory",
          "type": "graph",
          "yAxes": [{"format": "bytes", "min": 0}]
        }
      ],
      "showTitle": true,
      "title": "Memory Metrics",
      "titleSize": "h6"
    },
    {
      "collapse": false,
      "height": 250,
      "panels": [
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 12,
          "legend": {"show": true},
          "lines": true,
          "span": 3,
          "targets": [
            {"expr": "jvm_threads_live_threads", "legendFormat": "Live", "refId": "A"},
            {"expr": "jvm_threads_daemon_threads", "legendFormat": "Daemon", "refId": "B"},
            {"expr": "jvm_threads_peak_threads", "legendFormat": "Peak", "refId": "C"}
          ],
          "title": "JVM Threads",
          "type": "graph",
          "yAxes": [{"format": "short", "min": 0}]
        },
        {
          "datasource": "Prometheus",
          "fill": 1,
          "id": 13,
          "legend": {"show": true},
          "lines": true,
          "span": 3,
          "targets": [
            {"expr": "jvm_threads_states_threads", "legendFormat": "{{state}}", "refId": "A"}
          ],
          "title": "Thread States",
          "type": "graph",
          "yAxes": [{"format": "short", "min": 0}]
        },
        {
