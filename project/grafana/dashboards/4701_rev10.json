          "fill": 1,
          "id": 14,
          "legend": {"show": true},
          "lines": true,
          "span": 3,
          "targets": [
            {"expr": "system_load_average_1m", "legendFormat": "Load 1m", "refId": "A"},
            {"expr": "system_cpu_count", "legendFormat": "CPU Count", "refId": "B"}
